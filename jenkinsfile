node {
    stage('SCM'){
        checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/amanandrade/jenkins-jobs/']]])
    }
    stage('Build'){
        echo 'Building...'
    }
    stage('Test'){
        echo 'Testing...'
    }
    stage('Deploy'){
        echo 'Deploying...'
    }
}